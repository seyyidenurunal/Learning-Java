package DegiskenKavrami;

public class VeriTipiDönüşümleri {
    public static void main(String[] args) {
    
        int sayi = 10;
        double osayi = 120.12;
        
/*
        osayi = sayi;
        System.out.println("ondalıklı sayı değeri : " + osayi);
 */
 
        sayi = (int)osayi;
        System.out.println("intiger sayı değeri : " + sayi);
        

        byte bytesayi = 5;
        
        bytesayi = (byte)sayi;
        System.out.println("byte sayı değeri : " + bytesayi);
        
        

    }
}
